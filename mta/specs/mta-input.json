{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "repositoryURL": {
      "type": "string",
      "pattern": "^(git|http|https)://.+$"
    },
    "backstageUser": {
      "type": "string"
    },
    "backstageGroup": {
      "type": "string"
    }
  },
  "required": [
    "repositoryURL"
  ]
}

